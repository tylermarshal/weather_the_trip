<%= form_for @destination do |f| %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :latitude %>
  <%= f.text_field :latitude %>

  <%= f.label :longitude %>
  <%= f.text_field :longitude %>

  <%= f.label :population %>
  <%= f.text_field :population %>

  <%= f.submit %>

<% end %>
